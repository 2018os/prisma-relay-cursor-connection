datasource sqlite {
  provider = "sqlite"
  url      = "file:./dev.db"
}

generator client {
  provider = "prisma-client-js"
}

model Todo {
  id          String  @id
  text        String
  isCompleted Boolean @default(false)
}

model User {
  id           Int       @id @default(autoincrement())
  email String @unique

  username String
  isAdmin  Boolean? @default(false)

  @@unique([username, isAdmin])
}
